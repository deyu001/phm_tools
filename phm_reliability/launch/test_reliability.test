<launch>


  <node name="reliability_node" pkg="phm_reliability" type="reliability_node.py" output="screen">
      <rosparam file="$(find phm_reliability)/params/gui_test1.yaml" command="load"/>
      <rosparam file="$(find phm_reliability)/params/gui_test2.yaml" command="load"/>
      <rosparam file="$(find phm_reliability)/params/gui_test3.yaml" command="load"/>
