<launch>

  <node name="reliability_node" pkg="phm_reliability" type="reliability_node.py" output="screen">
      <rosparam file="$(find phm_reliability)/params/reliability_system_parameters.yaml" command="load"/>
      <rosparam file="$(find phm_reliability)/params/reliability_info_parameters.yaml" command="load"/>
      <rosparam file="$(find phm_reliability)/params/reliability_type_parameters.yaml" command="load"/>
      <rosparam file="$(find phm_reliability)/params/reliability_count_parameters.yaml" command="load"/>
  </node>
  

</launch>
